

<ul id="accordion">
<% increment = 1 %>
  <% @all_talks.each do |talk| %>
    <li class="one-talk" id="one-talk">
        <a href="#anchor-link<%= increment %>" class="talk-header">
        <h4 class="ui-accordion-header-icon"><%= talk["first_name"] %> <%= talk["last_name"] %>: <%= talk["talk_title"] %>
        </h4>
      </a>
      <p class="talk-body" id="anchor-link<%= increment %>"><%= talk["description"] %></p>
      <% increment += 1 %>
    </li>
  <% end %>
</ul>

